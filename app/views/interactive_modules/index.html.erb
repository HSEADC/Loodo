<p id="notice"><%= notice %></p>

<h1>interactive_modules</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @interactive_modules.each do |interactive_module| %>
      <tr>
        <td><%= interactive_module.name %></td>
        <td><%= interactive_module.description %></td>
        <td><%= link_to 'Show', interactive_module %></td>
        <td><%= link_to 'Edit', edit_interactive_module_path(interactive_module) %></td>
        <td><%= link_to 'Destroy', interactive_module, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New interactive_module', new_interactive_module_path %>
